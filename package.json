{
  "name": "openclaw-hipocampus",
  "version": "0.1.0",
  "description": "OpenClaw memory plugin powered by Hippocampus",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "openclaw": {
    "extensions": ["./dist/index.js"]
  },
  "files": ["dist", "openclaw.plugin.json", "README.md"],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
